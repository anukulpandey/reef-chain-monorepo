{
  "name": "react-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/react-lib/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/react-lib",
        "main": "packages/react-lib/src/index.ts",
        "tsConfig": "packages/react-lib/tsconfig.lib.json",
        "assets": ["packages/react-lib/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs react-lib {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/react-lib/jest.config.ts"
      }
    }
  },
  "tags": []
}
